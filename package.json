{
  "name": "casethread-poc",
  "version": "0.1.0",
  "description": "CLI proof of concept for generating legal documents using OpenAI and template system",
  "main": "dist/main/index.js",
  "bin": {
    "casethread-poc": "./dist/index.js"
  },
  "directories": {
    "doc": "docs"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.8.1",
    "@electron-forge/maker-deb": "^7.8.1",
    "@electron-forge/maker-rpm": "^7.8.1",
    "@electron-forge/maker-squirrel": "^7.8.1",
    "@electron-forge/maker-zip": "^7.8.1",
    "@electron-forge/plugin-vite": "^7.8.1",
    "@tailwindcss/typography": "^0.5.16",
    "@types/jest": "^29.5.14",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.10.6",
    "@types/pdfkit": "^0.17.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "autoprefixer": "^10.4.21",
    "electron": "^37.2.0",
    "electron-devtools-installer": "^4.0.0",
    "eslint": "^9.17.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "postcss": "^8.5.6",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.4.17",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "vite": "^7.0.3",
    "vite-plugin-static-copy": "^3.1.1"
  },
  "scripts": {
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "cli": "ts-node src/index.ts",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prebuild": "rm -rf dist",
    "postbuild": "chmod +x dist/index.js",
    "electron:dev": "electron-forge start",
    "electron:package": "electron-forge package",
    "electron:make": "electron-forge make",
    "electron:publish": "electron-forge publish",
    "electron:build": "npm run build:renderer && npm run build:main && npm run build:preload",
    "build:main": "vite build --config vite.main.config.ts",
    "build:preload": "vite build --config vite.preload.config.ts",
    "build:renderer": "vite build --config vite.renderer.config.ts"
  },
  "keywords": [
    "legal",
    "cli",
    "openai",
    "document-generation"
  ],
  "author": "",
  "license": "ISC",
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@heroui/react": "^2.7.11",
    "@langchain/core": "^0.3.62",
    "@langchain/langgraph": "^0.3.7",
    "@langchain/openai": "^0.5.18",
    "@types/uuid": "^10.0.0",
    "chalk": "^5.4.1",
    "chromadb": "^3.0.6",
    "commander": "^13.0.0",
    "dotenv": "^16.4.7",
    "electron-squirrel-startup": "^1.0.1",
    "framer-motion": "^12.23.1",
    "js-yaml": "^4.1.0",
    "openai": "^4.78.1",
    "ora": "^8.1.1",
    "pdfjs-dist": "^3.11.174",
    "pdfkit": "^0.17.1",
    "react": "^19.1.0",
    "react-arborist": "^3.4.3",
    "react-dom": "^19.1.0",
    "react-pdf": "^7.7.0",
    "uuid": "^11.1.0",
    "winston": "3.17.0"
  }
}
